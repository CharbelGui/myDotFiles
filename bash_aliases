# General alias

alias sl="ls"
alias lh="ls -alFh"
alias ..="cd .."
alias cd..="cd .."
alias cd-="cd -"
alias df="df -Th --total"
alias du="du -sh *"
alias free="free -h"
alias histg="history | grep"

